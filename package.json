{
  "name": "mail-feedback",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "9.7.1",
    "npm": "5.6.0"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
